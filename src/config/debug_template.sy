#package debug_temp : pkg;

#include defaults.sy;
#include io;

#if debug;
#raise "Unable to run debugger.";
#endif ;
#define debug

//default template for debug.sy file
constructor debugger();

func bool debug() {

  $d = new debugger({start: 0})

  with(io.write(d, "log.txt"));
  return true;
}

func revert($version : int = 1) {
  if(version == 1) {
    pass;
  }
  throw false;
}
