func str read($count : int) {
  $string : str;
  do(count) {
    string += __STDIN__();
  }
  return string;
}

func str readLine($delimiter = '\n') {
  $line : str;
  $character = '';
  while(character != delimiter) {
    character = char(read(1));
    line += character;
  }
  return line;
}

macro write($string : str) {
  for($elem in string) {
    __STDOUT__(elem);
  }
}
