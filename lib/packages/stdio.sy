func str read($count : int) {
  $string : str;
  do(count) {
    string += _SSLM002();
  }
  return string;
}

func str readLine($delimiter = '\n') {
  $line : str;
  $character = '';
  while(character != delimiter) {
    character = char(read(1));
    line += character;
  }
  return line;
}

func void write($string : str) {
  for($elem in string) {
    _SSLM001(elem);
  }
}
